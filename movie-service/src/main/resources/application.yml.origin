server:
  port: 8080

spring:
  application:
    name: movie-service
  data:
    redis:
      host: localhost
      port: 6379

eureka:
  client:
    service-url:
      defaultZone: http://localhost:8761/eureka

header:
  api:
    key: <GET FROM https://kinopoisk.dev/>

logging:
  level:
    org:
      hibernate:
        SQL: debug
        type:
          descriptor:
            sql: trace
  pattern:
    console: "%C{1.} [%-5level] %d{HH:mm:ss} - %msg%n"



